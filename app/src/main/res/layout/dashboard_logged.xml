<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DashboardLoggedActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:gravity="center">
        <Button
            android:layout_width="750px"
            android:layout_height="150px"
            android:text="Cadastrar Produto"
            android:onClick="btn_enter_frmcadprod"/>
        <Button
            android:layout_width="750px"
            android:layout_height="150px"
            android:text="Consultar Produto"
            android:onClick="btn_enter_frmconsprod"/>
        <Button
            android:layout_width="750px"
            android:layout_height="150px"
            android:text="Cadastrar Lojista"
            android:onClick="btn_enter_frmcadloji"/>
        <Button
            android:layout_width="750px"
            android:layout_height="150px"
            android:text="Consultar Lojista"
            android:onClick="btn_enter_consloji"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>